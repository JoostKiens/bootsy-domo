{
  "name": "bootsy-domo",
  "version": "1.0.0",
  "description": "Domotica for bootsy",
  "main": "server.js",
  "scripts": {
    "build": "npm run build:clean && npm run build:python && npm run build:server && npm run build:client",
    "build:clean": "rimraf dist/* &&  ([ -d dist ] || mkdir dist)",
    "build:client": "NODE_ENV=production webpack -p --config build/webpack.config.js --bail",
    "build:server": "NODE_ENV=production babel ./server --out-dir ./dist/server",
    "build:python": "cp -R ./*.py ./dist/",
    "serve": "NODE_ENV=production node dist/server/index.js",
    "start": "NODE_ENV=development node server/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JoostKiens/bootsy-domo.git"
  },
  "keywords": [
    "CHIP",
    "Arduino",
    "Express"
  ],
  "author": "Joost Kiens",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JoostKiens/bootsy-domo/issues"
  },
  "homepage": "https://github.com/JoostKiens/bootsy-domo#readme",
  "dependencies": {
